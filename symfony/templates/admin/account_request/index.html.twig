{% extends 'admin.base.html.twig' %}

{% block title %}AccountRequest index{% endblock %}

{% block body %}
    <h1>AccountRequest index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Company</th>
                <th>Role</th>
                <th>Message</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for account_request in account_requests %}
            <tr>
                <td>{{ account_request.firstName }}</td>
                <td>{{ account_request.lastName }}</td>
                <td>{{ account_request.email }}</td>
                <td>{{ account_request.phone }}</td>
                <td>{{ account_request.company }}</td>
                <td>{{ account_request.type }}</td>
                <td>{{ account_request.message }}</td>
                <td>
                    <a href="{{ path('app_admin_account_request_show', {'id': account_request.id}) }}">show</a>
                    {{ include('admin/account_request/_validate_form.html.twig') }}
                    {{ include('admin/account_request/_delete_form.html.twig') }}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
