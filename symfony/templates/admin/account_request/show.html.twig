{% extends 'admin.base.html.twig' %}

{% block title %}AccountRequest{% endblock %}

{% block body %}
    <h1>AccountRequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ account_request.id }}</td>
            </tr>
            <tr>
                <th>First_name</th>
                <td>{{ account_request.firstName }}</td>
            </tr>
            <tr>
                <th>Last_name</th>
                <td>{{ account_request.lastName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ account_request.email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ account_request.phone }}</td>
            </tr>
            <tr>
                <th>Company</th>
                <td>{{ account_request.company }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ account_request.type }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ account_request.message }}</td>
            </tr>
            <tr>
                <th>IsValid</th>
                <td>{{ account_request.isValid ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_account_request_index') }}">Back to list</a>

    <a class="wb-btn-edit btn" href="{{ path('app_admin_account_request_validate', {'id': account_request.id}) }}">validate</a>

    {{ include('admin/account_request/_delete_form.html.twig') }}
{% endblock %}